{
  "name": "@sqlrooms/bigquery",
  "version": "0.1.0",
  "description": "BigQuery IO utilities for SQLRooms applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.0.0",
    "apache-arrow": "^18.0.0",
    "zod": "^3.22.0"
  },
  "peerDependencies": {
    "@sqlrooms/duckdb": "*",
    "@sqlrooms/room-shell": "*",
    "express": "^4.18.0",
    "immer": "^10.2.0 || ^11.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "@sqlrooms/duckdb": {
      "optional": true
    },
    "@sqlrooms/room-shell": {
      "optional": true
    },
    "immer": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "express": "^5.2.1",
    "immer": "^11.1.3",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "bigquery",
    "sqlrooms",
    "duckdb",
    "arrow"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/replicahq/sqlrooms-bigquery.git"
  }
}
